# Environment Configuration for ATSReflectumAgentStarterPack
# Copy this file to .env and fill in your actual values

# LLM Service Configuration
LLM_PROVIDER=lmstudio
LMSTUDIO_LLM_PROXY_URL=http://127.0.0.1:1234
LMSTUDIO_LLM_MODEL_NAME=model:1

# Alternative LLM Providers (uncomment to use)
# LLM_PROVIDER=google
# GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_MODEL=gemini-2.0-flash

# LLM_PROVIDER=ollama
# OLLAMA_BASE_URL=http://localhost:11434
# OLLAMA_MODEL=llama2

# Database Configuration
DATABASE_PATH=chat.db

# Qdrant Vector Database (Optional - for advanced RAG features)
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION_NAME=PierwszaKolekcjaOnline
QDRANT_API_KEY=

# Embedding Service Configuration
EMBEDDING_PROVIDER=lmstudio
LMSTUDIO_PROXY_URL=http://127.0.0.1:8123
LMSTUDIO_MODEL_NAME=model:10

# Alternative Embedding Providers (uncomment to use)
# EMBEDDING_PROVIDER=huggingface
# HUGGINGFACE_MODEL_NAME=all-MiniLM-L6-v2

# EMBEDDING_PROVIDER=google
# GOOGLE_EMBEDDING_MODEL=textembedding-gecko@001

# EMBEDDING_PROVIDER=openai
# OPENAI_API_KEY=your_openai_api_key
# OPENAI_EMBEDDING_MODEL=text-embedding-ada-002

# Cache Service Configuration
CACHE_PROVIDER=memory
CACHE_SIZE=1000
CACHE_TTL=3600

# Search Service Configuration
SEARCH_PROVIDER=local
SEARCH_INDEX_NAME=default

# Application Configuration
PORT=8080
DEBUG=true
LOG_LEVEL=INFO

# Optional: Redis Configuration (if using Redis cache)
# REDIS_URL=redis://localhost:6379

# Optional: Elasticsearch Configuration (if using Elasticsearch search)
# ELASTICSEARCH_URL=http://localhost:9200
# ELASTICSEARCH_API_KEY=
